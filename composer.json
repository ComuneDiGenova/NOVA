    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "composer/installers": true,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },

        "drupal-core-project-message": {
            "include-keys": ["homepage", "support"],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",

                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
                          "enable-patching": true,
	"patches": {
	   "drupal/business_rules" : {
	   "Show Message (ShowMessage) does not work": "https://www.drupal.org/files/issues/2021-04-30/3211660-showmessage_does_not_work.patch",
	   "https://www.drupal.org/project/business_rules/issues/3202890": "https://www.drupal.org/files/issues/2022-04-06/send-email-action-access-array-offset-on-value-of-type-null-3202890-3.patch",
	  "add value to multi-value field": "https://www.drupal.org/files/issues/2022-04-11/set-multiple-fields-value-action-3095392-11.patch",
	  "Variables placed in the email body aren't replaced by their actual value" : "https://www.drupal.org/files/issues/2020-01-21/collect_variables-3056302-11.patch",
	  "Exception: The position 0, 0 is not empty.":"https://www.drupal.org/files/issues/2022-05-04/exception-flowchart-matrix-position-not-empty-3277915-3.patch",
	   "cannot add an action to a condition": "https://www.drupal.org/files/issues/2021-05-15/cannot_add_action-3200711-29.patch"
	   },
	   "drupal/form_mode_control" : {
	   "Unable to deny Default form mode": "https://www.drupal.org/files/issues/2021-08-30/form_mode_control-default-3202239-4.patch",
	   "Access to admin/people view, reset configuration to default":"https://www.drupal.org/files/issues/2021-12-27/access-to-admin-people-view-reset-configuration-to-default--3240682.patch"
	   }       
,
           "drupal/scheduler_field" : {
           "not working correctly with more than one cron processing activated plugin": "https://www.drupal.org/files/issues/2021-11-23/not_working_correctly_with_more_than_one_cron_processing_activated_plugin-3250808-002.patch",
           "Add ability to hide type selector and end date on widget": "https://www.drupal.org/files/issues/2021-11-23/scheduler_field-hide_form_elements-3201753-3.patch",
           "Scheduler field plugin Publication - timezone handling": "https://www.drupal.org/files/issues/2021-11-22/scheduler_field_plugin_publication_-_timezone_handling-3250384-003.patch"
           }

	}}
}
